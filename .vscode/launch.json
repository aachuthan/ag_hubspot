{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Dry Run Contacts",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "--object",
                "contacts",
                "--count",
                "1",
                "--dry-run"
            ]
        },
        {
            "name": "Python: Insert Contacts (Live)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "--object",
                "contacts",
                "--count",
                "1"
            ]
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}